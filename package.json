{
  "name": "fseh",
  "version": "0.0.0-semantically-released",
  "description": "A minimal Finite State Event Handler for JavaScript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "watch:test": "npm t -- -w",
    "test": "mocha src/*.test.js",
    "cover": "nyc --reporter=lcov --reporter=text npm t",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0xfede/fseh.git"
  },
  "keywords": [
    "fsm",
    "finite",
    "state",
    "machine",
    "event",
    "handler",
    "typescript",
    "javascript"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.ts",
    "README.md",
    "LICENSE"
  ],
  "author": "Federico Pinna <fpinna@vivocha.com> (http://www.vivocha.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/0xfede/fseh/issues"
  },
  "homepage": "https://github.com/0xfede/fseh#readme",
  "devDependencies": {
    "chai": "4.1.2",
    "commitizen": "2.9.6",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "2.0.0",
    "ghooks": "2.0.0",
    "mocha": "3.5.3",
    "nyc": "11.2.1",
    "semantic-release": "8.0.3",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0",
    "typescript": "2.5.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run build && npm run cover && npm run check-coverage"
    }
  }
}
